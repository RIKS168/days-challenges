import java.util.* ;
import java.io.*; 
public class Solution {
	public static void selectionSort(int arr[], int n) {
		for(int i=0;i<n;i++){
			int smallest_var=i;
			for(int j=i+1;j<n;j++){
				if(arr[smallest_var]>arr[j]){
					smallest_var=j;
				}
			}
			int temp=arr[smallest_var];
			arr[smallest_var]=arr[i];
			arr[i]=temp;
		}
	}
}
