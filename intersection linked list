public class Solution {
    public ListNode getIntersectionNode(ListNode headA, ListNode headB) {
      ListNode temp1=headA;
      ListNode temp2=headB;
        int len1=0;
        while(temp1!=null){
        temp1=temp1.next;
        len1++;
    }

    int len2=0;
    while(temp2!=null){
        temp2=temp2.next;
        len2++;

    }
temp1=headA;
temp2=headB;
if(len1>len2){
    int steps=len1-len2;
    for(int i=1;i<=steps;i++){
        temp1=temp1.next;
}
    }
    else{
        int steps=len2-len1;
        for(int i=1;i<=steps;i++){
            temp2=temp2.next;
        }
    }
   while(temp1!=temp2){
       temp1=temp1.next;
       temp2=temp2.next;
   }
    return temp1;
   }
    
    }
            
        
    
    
